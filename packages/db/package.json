{
  "name": "@botking/db",
  "version": "0.0.0",
  "private": true,
  "description": "Database package with Prisma",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "test": "echo 'test'",
    "db:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@botking/logger": "workspace:*",
    "@prisma/client": "^5.19.1",
    "prisma-zod-generator": "^1.20.3",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@types/node": "^22.8.7",
    "prisma": "^5.19.1",
    "tsx": "^4.19.1",
    "typescript": "^5.9.2"
  }
}
