{
  "name": "@botking/dto",
  "version": "1.0.0",
  "description": "TypeScript DTOs for CRUD operations on Botking artifacts using database repositories",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "dto",
    "database",
    "crud",
    "artifacts",
    "botking"
  ],
  "dependencies": {
    "@botking/db": "workspace:*",
    "@botking/artifact": "workspace:*",
    "@botking/validator": "workspace:*",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md"
  ]
}
